<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.freeman.mapper.FreeDictMapper">
    <select id="getFreeDict" resultType="com.freeman.properties.FreeDict">
        select ${dictLabel},${dictValue}
        from ${dictTableName}
        where ${dictType}=#{typeValue}
    </select>
</mapper>
